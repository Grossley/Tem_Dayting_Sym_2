        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Depth and Movement
if targetRoom == 0
{
    if instance_exists(obj_black_bars)
    {
        if (global.runDialogue==false &amp;&amp; global.runCutscene==false &amp;&amp; obj_black_bars.fadeAmount &lt;= 0.1)
        {
            script_execute(state);
            event_inherited();
        }
    }
    
    depth =-y;
    
    if (global.runCutscene==true)
    {
        scr_cutscene_main()
    }
    
    //timer
    if (dialogueDelay&gt;0)
    {
        dialogueDelay -=1;
    }
    
    //collision
    if collision_point(x,y,obj_trigger_nocollision,false,true) &amp;&amp; (keyboard_check_pressed(ord('Z')) || gamepad_button_check_pressed(0,gp_face1))
    {
        if (global.runDialogue==false &amp;&amp; dialogueDelay == 0)
        {
            ///Stop movement
            image_index = 0;
            image_speed = 0;
            
            //Set assignment for functions
            global.cutsceneWait = 0;
            global.cutsceneNumber = 0;
            
            inst = instance_nearest(x, y, obj_trigger_nocollision);
            //Perform creation event
            event_perform_object(inst,ev_create,0)
            
            //Set the cutscene
            global.cutscene = inst.cs
            global.cutsceneLength = array_height_2d(global.cutscene);
            
            global.runCutscene=true;
            properCol=false;
        }
    }
}
else
{
    ///Stop movement
    image_index = 0;
    image_speed = 0;
}


</string>
