            <kind>1</kind>
            <string>///Draw

//draw background
draw_set_color(c_black)
draw_set_alpha(1)
draw_rectangle(0,0,1280,720,false)

//draw text
draw_set_alpha(alp2)
draw_set_color(c_white)
draw_set_font(fnt_credits)
draw_set_valign(fa_top)
draw_set_halign(fa_center)
draw_text_ext(640,760-pos,text,20,1280)

if alp &lt; 1 &amp;&amp; timer == 180
{
    alp += 0.01
}
else if alp &gt;= 1 &amp;&amp; timer == 180
{
    timer --
}

if timer &lt; 180
{
    timer--
}
if timer &lt; 0 &amp;&amp; alp &gt; 0
{
    alp -= 0.01
}

draw_set_alpha(alp)
draw_text(640,260,"Tem Dayting Sym 2")
draw_set_alpha(1)

if timer &lt;= 0
{
    if clickTimer &gt; 0
    {
        clickTimer --
        if clickFade &lt; 1 &amp;&amp; clickTimer &lt; 240
        {
            clickFade += 0.01
        }
    }
    if clickFade &gt; 0 &amp;&amp; clickTimer &lt; 2
    {
        clickFade -= 0.01
    }
    if clickTimer &lt; 240
    {
        draw_set_alpha(clickFade)
        draw_text(1100,10,"Right-click to speed up#Left-click to reverse")
        draw_set_alpha(1)
    }
    
    if !audio_is_playing(mus_vn_edit)
    {
        audio_play_sound(mus_vn_edit,10,true)
        audio_sound_gain(mus_vn_edit,0,0)
        audio_sound_gain(mus_vn_edit,0.13 * global.music_vol,400)
    }
    if mouse_check_button(mb_left) || keyboard_check(vk_down)
    {
        spd = 4   
    }
    else if (mouse_check_button(mb_right) || keyboard_check(vk_up)) &amp;&amp; pos &gt; 96
    {
        spd = -4   
    }
    else
    {
        spd = 1
    }
    
    if string_height_ext(text,20,1280) &lt; pos - 360
    {
        spd = 0
        timer2--
    }
    
    if timer2 &lt;= 0
    {
        if music == true
        {
            audio_sound_gain(mus_vn_edit,0,200)
            music = false
        }
        alp2 -= 0.005
    }
    if alp2 &lt; 0.5 &amp;&amp; text !=text2
    {
        text = text2
    }
    if alp2 &lt;= 0
    {
        room_goto(rm_menu)
    }
    
    pos += spd
}
</string>
