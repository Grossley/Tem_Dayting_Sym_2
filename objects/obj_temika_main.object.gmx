if skipTimer &gt;=1
{
    skipTimer--;
}

scr_temika_main()

//spawn bg tems
if class_tems == true
{
    if instance_number(obj_classtem) &lt; 6
    {
        if (random(1.99) &lt; 1)
        {
            with instance_create(-140,random_range(520,560),obj_classtem)
            {
                spd = random_range(0.5,3)
                image_xscale = -1
                image_alpha = 0 + (y-520)/240
                if image_alpha &lt; 0.05
                    {
                        image_alpha = 0.05
                    }
                    if image_alpha &gt; 0.35
                    {
                        image_alpha = 0.35
                    }
            }
        }
        else
        {
            with instance_create(1420,random_range(520,560),obj_classtem)
            {
                spd = -random_range(0.5,3)
                image_xscale = 1
                image_alpha = 0 + (y-520)/240
                if image_alpha &lt; 0.05
                    {
                        image_alpha = 0.05
                    }
                    if image_alpha &gt; 0.35
                    {
                        image_alpha = 0.35
                    }
            }
        }
    }
}
