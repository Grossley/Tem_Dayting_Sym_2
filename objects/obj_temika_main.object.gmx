            <string>//Draw background
draw_set_alpha(1)
draw_set_color(c_black)
draw_rectangle(0,0,1280,720,false)

draw_sprite_stretched(background,0,0,0,1280,720)

if !(background == background2) 
{
    draw_set_alpha(bg2alpha)
    draw_sprite_stretched(background2,0,0,0,1280,720)
    
    if fade == "in" &amp;&amp; mouse_check_button_pressed(mb_left)
    {
        bg2alpha += alpharate;
        background = background2
        bg2alpha = 0;
        fade = ""
        scr_next_frame_temika()
    }
    else if fade == "in" &amp;&amp; bg2alpha &lt;= 1
    {
        bg2alpha += alpharate;
    }
    else if fade == "in" &amp;&amp; bg2alpha &gt;= 1 
    {
        bg2alpha += alpharate;
        background = background2
        bg2alpha = 0;
        fade = ""
        scr_next_frame_temika()
    }
    if  skipTimer &lt; 1 &amp;&amp; fade == "in" &amp;&amp; (mouse_check_button_pressed(mb_left) || keyboard_check(vk_control))
    {
        background = background2
        bg2alpha = 0;
        fade = ""
        skipTimer = skip;
        scr_next_frame_temika()
    }
    
    if fade == "out" &amp;&amp; mouse_check_button_pressed(mb_left)
    {
        bg2alpha -= alpharate;
        background = background2
        bg2alpha = 1;
        fade = ""
        scr_next_frame_temika()
    }
    else if fade == "out" &amp;&amp; bg2alpha &gt;= 0
    {
        bg2alpha -= alpharate;
        
    }
    else if fade == "out" &amp;&amp; bg2alpha &lt;= 0
    {
        bg2alpha -= alpharate;
        background = background2
        bg2alpha = 1;
        fade = ""
        scr_next_frame_temika()
    }
    
    if  skipTimer &lt; 1 &amp;&amp; fade == "out" &amp;&amp; (mouse_check_button_pressed(mb_left) || keyboard_check(vk_control))
    {
        background = background2
        bg2alpha = 1;
        fade = ""
        skipTimer = skip;
        scr_next_frame_temika()
    }
}
draw_set_alpha(1)
